  "therapyTargets": {
    "_description": "Common articulation error patterns and target minimal pairs",

    "fronting": {
      "_description": "Velar→Alveolar substitution: k→t, g→d",
      "targetPairs": ["voicingContrast.k-g", "placeContrast.p-t-k"]
    },

    "backing": {
      "_description": "Alveolar→Velar substitution: t→k, d→g",
      "targetPairs": ["voicingContrast.t-d", "placeContrast.p-t-k"]
    },

    "stopping": {
      "_description": "Fricative→Stop substitution: s→t, f→p, sh→t",
      "targetPairs": ["mannerContrast.s-sh", "voicingContrast.f-v"]
    },

    "gliding": {
      "_description": "Liquid→Glide substitution: r→w, l→w",
      "targetPairs": ["mannerContrast.w-r", "mannerContrast.w-l"]
    },

    "clusterReduction": {
      "_description": "Consonant cluster simplification",
      "targetPairs": ["clusterReduction.s-clusters", "clusterReduction.l-clusters", "clusterReduction.r-clusters"]
    },

    "finalConsonantDeletion": {
      "_description": "Word-final consonant deletion",
      "targetPairs": "finalConsonant"
    },

    "voicingErrors": {
      "_description": "Voiced/Voiceless confusion",
      "targetPairs": ["voicingContrast.p-b", "voicingContrast.t-d", "voicingContrast.k-g"]
    },

    "th-substitution": {
      "_description": "Th→F/S substitution (common in children)",
